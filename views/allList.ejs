<% layout("/layouts/boilerplate.ejs") %>
<body>
  <div class="main container main1 mx-auto">
    <div class="filters mx-auto">
      <div class="filter">
        <img
          src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQTeJpWkEJs9phW0g0wKR3j8kvN4Et2tGiWeQ&s"
          alt=""
        />
        <p>billboards</p>
      </div>
      <div class="filter">
        <img
          src="https://cdn-icons-png.flaticon.com/512/5893/5893948.png"
          alt="#"
        />
        <p>digital screens</p>
      </div>
      <div class="filter">
        <img
          src="https://cdn-icons-png.flaticon.com/512/1101/1101226.png"
          alt="#"
        />
        <p>transit</p>
      </div>
    </div>
    <div class="textToggle" id="textToggle1">
      <div class="form-check form-switch">
        <label class="form-check-label" for="flexSwitchCheckDefault"
          ><span class="all">With taxes</span
          ><span class="short">Taxes</span></label
        >
        <input
          class="form-check-input"
          type="checkbox"
          role="switch"
          id="flexSwitchCheckDefault"
        />
      </div>
    </div>
  </div>
  <div class="textToggle textToggle1 mt-3" id="textToggle">
    <div class="form-check form-switch">
      <label class="form-check-label" for="textgst">Taxes</label>
      <input
        class="form-check-input"
        type="checkbox"
        role="switch"
        id="textgst"
      />
    </div>
  </div>
  <form action="/listing/category" id="myForm">
    <input type="text" class="icons" name="category" style="display: none" />
  </form>
  <div class="container row row-cols-lg-3 row-cols-md-2 row-cols-sm-1 mt-3">
    <% for (let resultAll of result) {%>
    <div class="card">
      <a href="/listing/<%= resultAll._id %>">
        <img src="<%= resultAll.image.url %>" class="card-img-top" alt="#" />
        <div class="card-img-overlay like"></div>
        <div class="card-body">
          <b><p class="card-text"><%= resultAll.title %></p></b>
          <p class="card-text">
            &#x20B9;<%= resultAll.price.toLocaleString('en-IN') %>/day<i
              class="text-info"
              >&nbsp; &nbsp; +18% GST</i
            >
          </p>
        </div>
      </a>
    </div>
    <% } %>
  </div>
</body>
