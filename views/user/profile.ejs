<% layout("/layouts/boilerplate.ejs") %>
<h1 class="my-4">Welcome to Your Profile</h1>
<div class="d-flex justify-content-evenly my-4">
  <h2>Email: <%= user.username %></h2>
  <h2>Username: <%= user.email %></h2>
  <h2>Phone: <%= user.phone %></h2>
  <h2>UPI ID: <%= user.upi %></h2>
</div>
<div class="info">
  <p class="text-center my-4"><b>Your past reservation details</b></p>
</div>
<div class="table">
  <table class="table table-striped table-hover">
    <thead>
      <tr class="text-center">
        <th scope="col">Space Owner</th>
        <th scope="col">Space title</th>
        <th scope="col">Space Location</th>
        <th scope="col">CheckIn Space Date</th>
        <th scope="col">CheckOut Space Date</th>
        <th scope="col">Total Payment</th>
      </tr>
    </thead>
    <tbody>
      <% for(let allResult of result) {%>
      <tr class="text-center">
        <td><%= result2.owner.username %></td>
        <td><%= result2.title %></td>
        <td><%= result2.location %></td>

        <td><%= allResult.checkIn.toLocaleString() %></td>
        <td><%= allResult.checkOut.toLocaleString() %></td>
        <td><%= allResult.price %></td>
      </tr>
      <% } %>
    </tbody>
  </table>
</div>
